{
	"#region": {
        "prefix": "#region",
        "body": [
			"REM #region ${1:description}",
			"${TM_SELECTED_TEXT}",
			"REM #endregion"
        ],
        "description": "Folding Region"
    },
	"forto":
	{
		"prefix": "for",
		"body":[
			"Dim ${1:Counter}",
			"For ${1:Counter} = 1 to ${2:ArrayLength} step 1",
			"\tDim ${3:CurrentObject} = ${4:ArrayName}[${1:Counter}]",
			"\t",
			"Next"
		],
		"description": "Defines a step counter and uses in a For i to TestExp step # loop"
	},
	"dountil":
	{
		"prefix": "do",
		"body":[
			"Dim ${1:Flag} = False",
			"Do",			
			"\t",
			"Loop Until ${1:Flag}"
		],
		"description": "Defines loop that will get executed at least once and until condition is True"
	},
	"dowhile":
	{
		"prefix": "do",
		"body":[
			"Dim ${1:Flag} = True",
			"Do while ${1:Flag}",			
			"\t",
			"Loop"
		],
		"description": "Defines loop that will get excuted until the condition is False"
	},
	"ifel":
	{
		"prefix": "ifel",
		"body":[
			"If (${1:expression}) Then",
			"\t",
			"Else",
			"\t",
			"End If"			
		],
		"description": "Inserts and If-Else structure"
	}
}
	
 







	
	

