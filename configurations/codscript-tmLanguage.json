{
	"warning": "This file is overwritten by the ItemDefinitionParser utility using the codscript-tmLanguage.template.json file as its baseline",
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "codscript",
	"scopeName":"source.codscript",
	"fileTypes":["codscript"],
	
	"patterns": [
		{
			"comment": "Comments",
			"name": "markup.bold comment",
			"match": "(?i:REM\\s+.*)"
		},
		{
			"comment": "Strings",
			"name": "comment markup.italic",
			"begin": "\"",
			"end": "\""
		},
		{
			"comment": "Numbers",
			"name": "markup.bold constant.numeric",
			"match": "(?<=\\W)(\\-\\d+\\.\\d+|\\d+\\.\\d+|\\-\\.\\d+|\\.\\d+|\\-\\d+|\\d+)(?=\\W)"
		},
		{
			"comment": "Operators",
			"name": "markup.bold constant.regexp",
			"match": "\\+|\\-|\\*|/|\\>|\\<|=|,|\\.|\\[|\\]|\\(|\\)"
		},
		{
			"comment": "Named Constants",
			"name": "invalid",
			"match": "\\b((?i)MAPPATH_BACKUP|MAPPATH_BLOCKS|MAPPATH_CNC|MAPPATH_DATABASE|MAPPATH_DXF|MAPPATH_FILTER|MAPPATH_HOME|MAPPATH_IMAGES|MAPPATH_INSTALL|MAPPATH_ITEMS|MAPPATH_PARTS|MAPPATH_PROJECT|MAPPATH_REMNANTS|MAPPATH_REPORTS|MAPPATH_SCRIPTS|PI|EO_NEVER)\\b"
		},		
		{
			"comment": "Value Constants",
			"name": "support.type.property-name",
			"match": "\\b((?i)TRUE|FALSE|NULL|VOID|LOCK_USER|LOCK_OWNER|EXEC_DEFAULT|EXEC_WAIT|EXEC_SHOW_NORMAL|EXEC_SHOW_MAX|EXEC_SHOW_MIN|FORINPUT|FOROUTPUT|ISTEXT|UNICODETEXT|UTF8|UTF16|FILE_END|FILE_START|TIME_SECS|TIME_MINS|TIME_HOURS)\\b"
		},
		{
			"comment": "Control Keywords",
			"name": "markup.bold support.type.property-name",
			"match": "\\b((?i)IF|THEN|ELSE|ELSE IF|ELSEIF|END IF|ENDIF|WHILE|END WHILE|ENDWHILE|FOR|TO|STEP|NEXT|DO|UNTIL|LOOP|SELECT|CASE|END SELECT|ENDSELECT|FUNCTION|RETURN|END FUNCTION|ENDFUNCTION)\\b"
		},
		{
			"comment": "Special Keywords",
			"name": "keyword.control",
			"match": "(?<=^|\\s+)((?i)AND|OR|NOT|AS|IS|NEW|DIM|OBJECT|REQUIRES|INCLUDE|RUN|BREAKPOINT)(?=\\s+)"
		},
		{
			"comment": "Global Functions",
			"name": "markup.bold",
			"match": "\\b((?i)ACOS|ASIN|ATAN|COS|SIN|TAN|EXP|LOG|ABS|NUMBER|POW|SIGN|SQRT|SQR|ROUND|ROUNDDOWN|ROUNDUP|ASC|ASCII|CHR|GETFILEEXT|GETFILENAME|GETFILEPATH|INSTR|LEFT|LEN|LOWER|LTRIM|MID|RIGHT|RTRIM|SUBSTRING|TRIM|UPPER|WILDCARD|DEBUG|EXEC|INPUTBOX|QUERY|ERROR|OUTPUT)(?=\\()"
		},
		{
			"comment": "Passive Functions",
			"name": "markup.bold",
			"match": "\\b((?i)DEBUG|OUTPUT)\\b"
		},
		{
			"comment": "Objects",
			"name": "markup.bold string.regexp",
			"match": "(^|\\s|=)((?i)ARRAY|FILE|FILELOCATOR|ITEMSTRUCT|ITEM|TASK)\\b"
		},
		{
			"comment": "Object Collections",
			"name": "support.class markup.italic",
			"match": "(?<=\\.)((?i)AIRTURN|CONNECTOR|CUSTOMDATA|DAMPER|DIM|ENTRY|FILE|FOLDER|ITEM|LINK|OLDSTATUS|OPTION|PARTCUT|SEAM|SELECTION|SKINCONNECTOR|SKINSEAM|SPLITTER|STATUS|STIFFENER|SUBITEM)(?=\\[)"
		},
		{
			"comment": "Object Methods",
			"name": "markup.bold support.class",
			"match": "(?<=\\.)((?i)ADD|ADDALIAS|ADDAREA|ADDBOUGHTOUT|ADDCADBLOCK|ADDCUSTOMDATA|ADDDATABASEID|ADDDIM|ADDFLOW|ADDLINK|ADDOPTION|ADDORDER|ADDWEIGHT|BEGINPROGRESS|BITMAPFILE|CAN|CANDOUBLEWALL|CANROTARY|CLOSE|DELETE|DELETELINK|ENDLOCATION|ENDPROGRESS|INSERT|LEVEL|LOAD|OPEN|READBYTE|READCHAR|READINT|READLINE|READREAL|READSTRING|READWORD|REFRESHCOSTS|REMOVEALIAS|REMOVEAREA|REMOVEBOUGHTOUT|REMOVECADBLOCK|REMOVECUSTOMDATA|REMOVEDATABASEID|REMOVEDIM|REMOVEFLOW|REMOVEHOLES|REMOVEOPTION|REMOVEORDER|REMOVEWEIGHT|RENAME|SAVE|SCAN|SEEKENTRY|SEEKLINE|SETDEVNOTCUTFLAG|SETFLOW|SETOWNER|SETSTATUS|SETVERSION|UPDATE|WRITEBYTE|WRITECHAR|WRITEDXF|WRITEINT|WRITELINE|WRITEREAL|WRITESTRING|WRITEWORD)(?=\\()"
		},
		{
			"comment": "Object Properties",
			"name": "support.class",
			"match": "(?<=\\.)((?i)ABORTED|ACTIVE|AIRTURNS|ALIAS|ALT|ALTERNATE|ANNOTATION|AREA|BEADING|BITMAP|BOUGHTOUT|BOX|BUTTONALIAS|BUTTONCODE|CADBLOCK|CATALOGUE|CHANGED|CID|COILWIDTH|COLOUR|COMMENT|CONNECTORS|COSTBYLENGTH|COSTTYPE|COUNT|CUTTYPE|DAMPERS|DATABASEID|DATE|DATETIME|DBLOCK|DECOILER|DESCRIPTION|DIMS|DIMSIDE|DIMSIDELOCK|DOUBLEWALL|DRAWING|DWLOCK|ENTRIES|EOF|ETAG|EXISTS|EXTRAETIME|EXTRAETIMERATE|EXTRAETIMEUNITS|EXTRAFTIME|EXTRAFTIMERATE|EXTRAFTIMEUNITS|FABTABLE|FABTABLELOCK|FACING|FACINGLOCK|FIELD1|FIELD2|FILECOUNT|FILENAME|FIXRELATIVE|FLOWMAX|FLOWMIN|FOLDERCOUNT|GAUGE|GAUGELOCK|GROUP|GUID|GUID64|HANDLE|HASALIAS|HASAREA|HASBOUGHTOUT|HASCADBLOCK|HASCUSTOMDATA|HASCUSTOMDATAS|HASDATABASEID|HASDIMS|HASFLOW|HASOPTIONS|HASORDER|HASPRODUCT|HASSKEY|HASWEIGHT|HISTORY|ID|INFO|INSSPEC|INSTALLTABLE|INSTALLTABLELOCK|INSULATION|ISOPEN|ISPECLOCK|ISUNICODE|ITEMS|LASTACTIVATED|LENGTH|LIBRARY|LIFESPAN|LINKS|LOCKED|MANYOLDSTATUS|MATABRV|MATERIAL|MATERIALLOCK|MESSAGE|MODE|MODEL|NAME|NESTPRIORITY|NOTES|NUMBER|NUMVALUE|OPERATINGCOST|OPTIONS|ORDER|OWNER|PALLET|PARAM|PARTSCUT|PATH|PATNO|PCFSKEY|POSITION|PRICELIST|PRICETABLELOCK|PRODUCT|PROGRESS|PROJECT|QTY|REFERENCE|REVISION|ROTATION|SCALE|SEALANT|SEAMS|SECTION|SERVICE|SERVICETYPE|SKEY|SKINDECOILER|SKINGAUGE|SKINMATERIAL|SKINMATERIALLOCK|SKINSIDE|SMALLLENGTH|SPACING|SPECIFICATION|SPECLOCK|SPLITTERS|SPOOL|SPOOLCOLOUR|STATUS|STATUSES|STATUSLOCK|STDLENGTH|STIFFENERS|STQQTY|STRUCTURETYPE|SUBITEMS|SUPPORT|TARGET|TYPE|USERID|VALUE|VERSION|WEIGHT|WEIGHTLOCK|WIREGAUGE|ZONE)\\b"
		}
	]
}